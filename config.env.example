# MATDEV Bot Configuration
# Copy this file to config.env and update the values

# Bot Configuration
BOT_NAME=MATDEV
PREFIX=.
LOG_LEVEL=info

# Session Management (Auto-configured by launcher)
# WHATSAPP_SESSION_ID=session_xxxxx
# PHONE_NUMBER=
# OWNER_JID=
# AUTH_METHOD=1

# Features
ANTI_DELETE=true
ANTI_VIEW_ONCE=true
AUTO_RELOAD_PLUGINS=true
ENABLE_GAMES=true
ENABLE_MEDIA_TOOLS=true

# Access Control
OWNER_ONLY=true
ALLOWED_COMMANDS=

# Performance
MAX_UPLOAD=50
BAILEYS_LOG_LVL=silent

# External APIs (Optional)
OPENAI_API_KEY=
GOOGLE_API_KEY=
WEATHER_API_KEY=

# Advanced Features
HOT_RELOAD=true
ENABLE_PERFORMANCE_MONITORING=true
ALLOW_EXTERNAL_PLUGINS=true

# Web Interface (Optional)
ENABLE_WEB=false
WEB_PORT=5000
WEB_HOST=0.0.0.0