{
  "bot": {
    "name": "MATDEV",
    "version": "1.0.0",
    "commandPrefix": ".",
    "maxMessageLength": 4096,
    "loadingEmoji": "⏳",
    "successEmoji": "✅",
    "errorEmoji": "❌",
    "warningEmoji": "⚠️",
    "infoEmoji": "ℹ️"
  },
  "features": {
    "enableWebInterface": false,
    "autoReloadPlugins": true,
    "enableMediaDownload": true,
    "enableMessageArchive": true,
    "enableLoadingReactions": true,
    "enableGameMode": true,
    "enableAntiDelete": true,
    "enableMediaTools": false,
    "enablePerformanceMonitoring": true
  },
  "access": {
    "ownerOnly": true,
    "allowGameInputs": true,
    "allowExplicitCommands": true,
    "maxAllowedUsers": 10,
    "commandCooldown": 1000,
    "rateLimitEnabled": true
  },
  "storage": {
    "dataDirectory": "./data",
    "sessionsDirectory": "./sessions",
    "cacheTimeout": 300000,
    "autoSaveInterval": 60000,
    "maxCacheSize": 1000,
    "compressionEnabled": false
  },
  "media": {
    "maxFileSize": 52428800,
    "maxDownloadsPerHour": 100,
    "enableThumbnails": true,
    "allowedTypes": ["image", "video", "audio", "document", "sticker", "ptt"],
    "downloadDirectory": "./data/media",
    "tempDirectory": "./temp",
    "cleanupInterval": 86400000,
    "maxAge": 2592000000
  },
  "games": {
    "enableGames": true,
    "allowNonOwners": true,
    "maxPlayersPerGame": 10,
    "gameTimeout": 600000,
    "maxActiveGames": 5,
    "enableSpectators": false
  },
  "messaging": {
    "enableAutoReactions": true,
    "reactionTimeout": 30000,
    "maxMessageCache": 1000,
    "messageTTL": 1800000,
    "enableQuotedMessages": true,
    "enableMentions": true
  },
  "performance": {
    "enableMonitoring": true,
    "logLevel": "info",
    "maxMemoryUsage": 512,
    "cpuThreshold": 80,
    "responseTimeThreshold": 5000,
    "monitoringInterval": 30000
  },
  "plugins": {
    "autoDiscovery": true,
    "hotReload": true,
    "pluginTimeout": 10000,
    "maxPlugins": 20,
    "enableSandbox": false,
    "allowDynamicLoading": true
  },
  "security": {
    "validateJids": true,
    "enableRateLimit": true,
    "maxCommandsPerMinute": 10,
    "blockSuspiciousActivity": true,
    "enableAuditLog": true,
    "sessionTimeout": 3600000
  },
  "web": {
    "enabled": false,
    "port": 3000,
    "host": "0.0.0.0",
    "staticPath": "./web",
    "enableSocketIO": true,
    "corsEnabled": true,
    "authRequired": false
  },
  "logging": {
    "level": "info",
    "enableConsole": true,
    "enableFile": false,
    "logDirectory": "./logs",
    "maxFileSize": 10485760,
    "maxFiles": 5,
    "enableErrorReporting": true
  },
  "backup": {
    "enabled": false,
    "interval": 86400000,
    "maxBackups": 7,
    "backupDirectory": "./backups",
    "compressBackups": true,
    "includeMedia": false
  },
  "environment": {
    "userEnvFile": ".env",
    "systemEnvFile": ".env.system",
    "autoCreateEnv": true,
    "encryptSecrets": false,
    "defaultValues": {
      "NODE_ENV": "development",
      "ENABLE_WEB_INTERFACE": "false",
      "AUTO_RELOAD_PLUGINS": "true",
      "ENABLE_MEDIA_DOWNLOAD": "true",
      "ENABLE_MESSAGE_ARCHIVE": "true",
      "MAX_MEDIA_SIZE": "52428800",
      "MAX_MESSAGE_CACHE": "1000",
      "PLUGIN_TIMEOUT": "10000"
    }
  },
  "timeouts": {
    "initialization": 30000,
    "command": 10000,
    "plugin": 10000,
    "download": 30000,
    "upload": 30000,
    "game": 600000,
    "reaction": 30000
  },
  "limits": {
    "maxUsers": 1000,
    "maxChats": 100,
    "maxMessages": 10000,
    "maxMediaFiles": 1000,
    "maxPlugins": 20,
    "maxGames": 5
  },
  "cleanup": {
    "enableAutoCleanup": true,
    "cleanupInterval": 3600000,
    "maxCacheAge": 1800000,
    "maxLogAge": 604800000,
    "maxTempFileAge": 3600000,
    "maxInactiveGameAge": 86400000
  }
}